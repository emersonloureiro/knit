func main {
    expr1 = 1
    assert(expr1 == 1)
    expr2 = 1 + 1
    assert(expr2 == 2)
    expr3 = 1 + (1 + 3)
    assert(expr3 == 5)
    expr4 = 1 + 2 + 3 + 4
    assert(expr4 == 10)
    expr5 = (1 + 3 - (4 + 10) * (2 - 1))
    assert(expr5 == -10)
    expr6 = (2 * 5) - (3 / 2)
    assert(expr6 == 8.5)
    expr7 = (1 + 2) + (4)
    assert(expr7 == 7)
    expr8 = 3 - 10 * 2
    assert(expr8 == -17)
    expr9 = (3 - 10) * 2
    assert(expr9 == -14)
}